{
  "name": "reep.io",
  "version": "1.2.2",
  "description": "The official reep.io source code",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-bootstrap": "^2.2.0",
    "angulartics": "^1.3.0",
    "angulartics-piwik": "^1.0.4",
    "bootstrap": "3.3.7",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "pastejs": "^1.0.6",
    "peer": "^0.2.0",
    "peerjs": "^0.3.14",
    "url-loader": "^0.5.8"
  },
  "scripts": {
    "build-html": "rsync -r src/html/ public/html && rsync src/html/*.html public/",
    "build-js": "webpack -p",
    "build-media": "rsync -r src/media/ public/media",
    "build": "parallelshell 'npm run build-html' 'npm run build-media' 'npm run build-js'",
    "watch-html": "watch 'npm run build-html' 'src/html'",
    "watch-js": "webpack -w -p",
    "watch-media": "watch 'npm run build-media' 'src/media'",
    "watch": "parallelshell 'npm run watch-html' 'npm run watch-js' 'npm run watch-media'",
    "start": "parallelshell 'http-server public -a 0.0.0.0 -p 9001 -c-1' 'npm run watch'",
    "test": "karma start"
  },
  "author": "https://github.com/kodekraftwerk",
  "license": "GPL-2.0",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "bower": "^1.6.3",
    "css-loader": "^0.28.0",
    "http-server": "^0.8.5",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.12",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "parallelshell": "^2.0.0",
    "style-loader": "^0.16.1",
    "watch": "^0.16.0",
    "webpack": "^2.3.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "url-loader": "^0.5.8"
  },
  "subdomain": "kodekraftwerk-reepio",
  "engines": {
    "node": "0.12.x"
  }
}
